def isprime(n):
        if n==1:
            return False
        for i in range(2,n):
            if n%i==0:
                return False
        return True
n=int(input('enter n value:'))
l=[]
c=0
for i in range(n):
    x=int(input())
    l.append(x)
print(l)
for i in range(len(l)):
    if isprime(l[i]):
        c=c+1
print('count=',c)

OUTPUT:
enter n value:4
5
6
7
8
[5, 6, 7, 8]
count= 2
